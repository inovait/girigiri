{
  "name": "girigiri",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .local.env -- tsx src/migrate.ts",
    "dump:schema": "dotenv -e .env -- tsx src/sql_dump.ts",
    "check:migrations": "bash ./scripts/basic_migrations.sh",
    "check:migrations:diff": "bash ./scripts/migration_with_diff.sh",
    "build": "tsc",
    "migrate": "dotenv -e .env -- tsx src/migrate.ts",
    "docker:init-mig-database":"bash ./scripts/init_migration_database.sh",
    "docker:start": "docker compose --env-file .env up -d --build",
    "docker:reset": "bash ./scripts/docker_reset.sh",
    "docker:enter": "docker exec -it node-app /bin/sh",
    "docker:migrate": "docker run node-app node dist/migrate.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inovait/girigiri.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/inovait/girigiri/issues"
  },
  "homepage": "https://github.com/inovait/girigiri#readme",
  "dependencies": {
    "@types/typescript": "^0.4.29",
    "dotenv": "^17.2.1",
    "mysql2": "^3.14.3",
    "pino": "^9.7.0",
    "pino-pretty": "^13.1.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.0",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  }
}
